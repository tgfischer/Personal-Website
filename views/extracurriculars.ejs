<!DOCTYPE html>
<html>
<head>
	<!-- Standard Meta -->
	<meta charset="utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">

	<!-- Site Properities -->
	<title><%= title %></title>
	<link rel="stylesheet" type="text/css" href="/libs/semantic/semantic.min.css">
	<link rel="stylesheet" type="text/css" href="/css/styles.css">
</head>
<body>
	<!-- Page Contents -->
	<div class="pusher">
		<% include common/nav %>

		<div class="ui vertical inverted stripe segment title">
			<div class="ui middle aligned stackable grid container">
				<div class="row">
					<div class="column">
						<h1><%= header %></h1>
					</div>
				</div>
			</div>
		</div>

		<div class="ui vertical stripe segment">
			<div class="ui stackable grid container">
				<div class="center aligned row">
					<div class="column">
						<h1 class="ui header">Activities and Interests</h1>
						<div class="ui horizontal divider"><i class="user icon"></i></div>
					</div>
				</div>
				<div class="row">
					<div class="column">
						<table class="ui celled striped table">
							<thead>
								<tr>
									<th>Activity</th>
									<th>Date</th>
								</tr>
							</thead>
							<tbody>
								<% for (var i = 0; i < activities.length; i++) { %>
									<tr>
										<td><%= activities[i].activity %></td>
										<td><%= activities[i].date %></td>
									</tr>
								<% } %>
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>

		<div class="ui inverted vertical stripe segment">
			<div class="ui stackable grid container">
				<div class="center aligned row">
					<div class="column">
						<h1 class="ui inverted header">Community Involvement</h1>
						<div class="ui inverted horizontal divider"><i class="checkmark icon"></i></div>
					</div>
				</div>
				<div class="row">
					<div class="column">
						<table class="ui inverted celled striped table">
							<thead>
								<tr>
									<th>Activity</th>
									<th>Date</th>
								</tr>
							</thead>
							<tbody>
								<% for (var i = 0; i < community.length; i++) { %>
									<tr>
										<td><%= community[i].activity %></td>
										<td><%= community[i].date %></td>
									</tr>
								<% } %>
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>

		<div class="ui vertical stripe segment">
			<div class="ui stackable grid container">
				<div class="center aligned row">
					<div class="column">
						<h1 class="ui header">Awards and Accomplishments</h1>
						<div class="ui horizontal divider"><i class="star icon"></i></div>
					</div>
				</div>
				<div class="row">
					<div class="column">
						<table class="ui celled striped table">
							<thead>
								<tr>
									<th>Award or Accomplishment</th>
									<th>Date</th>
								</tr>
							</thead>
							<tbody>
								<% for (var i = 0; i < awards.length; i++) { %>
									<tr>
										<td><%= awards[i].award %></td>
										<td><%= awards[i].date %></td>
									</tr>
								<% } %>
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>

		<% include common/footer %>
	</div>

	<script src="https://code.jquery.com/jquery-1.11.3.min.js"></script>
	<script src="/libs/semantic/semantic.min.js"></script>
	<script src="/js/scripts.js"></script>

</body>

</html>
